#include <stdio.h>

int main()
{
    int arr1[100],n,i,j,k,arr2[200];
    printf("enter number of elements: ");
    scanf("%d", &n);
    printf("Enter %d elements: ", n);
    for (i=0;i<n;i++){
        scanf("%d", &arr1[i]);
    }
    for(i=0;i<n;i++){
        for(j=i+1;j<n;j++){
            if (arr1[i]==arr1[j]){
                arr2[k]=arr1[i];
                k++;
                break;
            }
        }
    }

    printf("array after deleting duplicate elements: ");
    for(i=0;i<k;i++){
        printf("%d ",arr2[i]);
    }
    return 0;
}
