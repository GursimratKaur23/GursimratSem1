#include <stdio.h>

int main() 
{
    FILE *file;
    char ch;
    if (file == NULL) {
        printf("File is null.");
        return 0;
    }
    file = fopen("ques2.txt", "r");
    ch = fgetc(file);

    if (ch == EOF)
        printf("The file is empty.");
    else
        printf("The file contains some text.");

    fclose(file);
    return 0;
}

