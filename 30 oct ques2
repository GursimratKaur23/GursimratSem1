#include <stdio.h>

int main() {
    FILE *paper;
    char line[200];

    paper = fopen("notes.txt", "r");

    if (paper == NULL) {
        printf("File not found.\n");
        return 1;
    }

    while (fgets(line, sizeof(line), paper)) {
        printf("%s", line);  
    }

    fclose(paper);
    return 0;
}
